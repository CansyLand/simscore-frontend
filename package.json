{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "concurrently \"npm run build-frontend\" \"npm run build-backend\"",
    "build-frontend": "next build",
    "build-backend": "cd api && pip3 install -r requirements.txt",
    "dev": "concurrently \"npm run dev-frontend\" \"npm run dev-backend\"",
    "dev-frontend": "next dev",
    "dev-backend": "cd api && python3 -m uvicorn index:app --reload",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-frontend": "next start -p ${PORT_FRONTEND-3000}",
    "start-backend": "cd api && uvicorn index:app --host 0.0.0.0 --port $PORT",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "chart.js": "^4.0.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.4.5",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "mpld3": "^0.5.10",
    "next": "^14.2.3",
    "nltk": "^0.0.1",
    "postcss": "^8.4.38",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-dropzone-textarea": "^1.2.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vercel": "^34.1.9"
  }
}
